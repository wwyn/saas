.topBanner{
    width: 100%;
}
.category-row{
    font-size: 14px;
    display: flex;
    align-items: stretch;
    overflow: hidden;
    margin-bottom: 45px;
}
.layout-left{
    width: 90px;
    background-color: #F8F8F8;
    overflow: hidden;
}
.layout-right{
    overflow: hidden;
}
.col-left{
    background: #ececec;
}
.col-left .c-item{
    line-height: 40px;
    padding-left: 7.5px;
    padding-right: 7.5px;
    white-space: nowrap;
    text-overflow:ellipsis;
    overflow: hidden;
    border-right: none;
    text-align: center;
    background-color: #F8F8F8;
    border-left:2px solid transparent;
    position: relative;
}
.cat-cartcount{
    position: absolute;
    top: 0;
    right: 4px;
}
.col-left .c-item:first-child{
    border-top: none;
}
.col-left .c-item.active{
    background: #fff;
    border-left-color: #E64340;
}
.fix-foot-bar{
    background: #efefef;
    height: 45px;
    width: 100%;
    position: fixed;
    display: flex;
    justify-content: space-between;
    bottom: 0;
    left: 0;
    z-index: 100;
}
.fix-foot-bar navigator{
    line-height: normal;
    padding: 0;
    margin: 0;
    height: 25px;
}
.foot-left{
    text-align:center;
    padding: 5px 10px 0 10px;
}
/* .foot-left contact-button{
    display: block;
} */
.product-online-comment::before,
.product-online-comment::after{
    border: none;
}
.icon-tip{
    font-size: 12px;
    color: #999;
    display: block;
    white-space: nowrap;
}
.foot-right{
    width: 100%;
    text-align: right;
}
.buy-area{
    display: flex;
    align-items: center;
    flex-direction:row-reverse;

}
.buy-area button{
    width: 50%;
    height: 45px;
    line-height: 45px;
    margin: 0;
    font-size: 14px;
    display: inline-block;
    border-radius: 0;
}
.buy-area button::before,.buy-area button::after{
    display: none;
    border-radius: 0;
}
.cart-quick-view{
    position: relative;
}
.fix-foot-bar .icon{
    height: 20px;
    width: 20px;
}
.cart-quick-view .cartcount{
    position: absolute;
    top:-8px;
    left:23px;
}
.co-item .weui-media-box__hd{
    width: 30%;
    height: 90px;
    line-height: 90px;
    overflow: hidden;
}
.co-item .weui-media-box__hd image{
    width: 100%;
    height: 100%;
    margin: 0;
}
.co-item .weui-media-box__bd{
    width: 70%;
}
.co-item{
    padding-right: 15px;
    position: relative;
}
.cat-headbar{
    padding-left: 15px;
}
.co-name{
    color: #333;
    margin-bottom: 10px;
    height: 17px;
    font-size: 16px;
}
.co-brief{
    padding-bottom: 18px;
    height: 14px;
}
.co-price{
    line-height: 22px;
    color: #f60;
    font-weight: lighter;
    font-stretch: ultra-condensed;
    font-size: 14px;
}
.co-price text{
    color: #999;
}
.addcart-block{
    position: absolute;
    right: 15px;
    bottom: 15px;
}
.pq-minus{
    width: 20px;
    height: 20px;
    line-height: 20px;
    vertical-align: middle;
    border:1px solid #E64340;
    align-items: center;
    justify-content: center;
    display: flex;
    border-radius: 100%;
    color: #ffffff;
    background-color: #E64340;
    font-size: 14px;
}
.input{
    width: 20px;
    text-align: center;
    font-size:14px;
    font-weight:normal;
}
.co-inline{
    display: flex;
    align-items: center;
    justify-content:space-between;
}
.weui-media-box:before{
    border-top: none;
}
.weui-panel:before{
    border-top: none;
}
.weui-panel:after{
    border-bottom: none;
}
.icon-cart-empty{
    background-image: url(data:image/svg+xml;base64,);
}
.icon-cart-fill{
    background-image: url(data:image/svg+xml;base64,);
}
.goods-product-warrper{
    position: fixed;
    z-index: 999;
    top: 0;
    left: 0;
    right: 0;
}
.warpper-background{
    position: relative;
    top: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.5);
}
.goods-product-content{
    position: absolute;
    top: 90px;
    left: 25px;
    background-color: #ffffff;
    border-radius: 4px;
    z-index: 9999;
    overflow: hidden;
}
.goods-image-warpper{
    position: relative;
}
.goods-brief{
    overflow: hidden;
    font-size: 14px;
    color: #999;
    height: 20px;
    padding: 0 15px;
}
.goods-name{
    padding: 8px 15px 0;
    height: 27px;
    overflow: hidden;
}
.handle-block{
    display: flex;
    justify-content: space-between;
    padding:0 15px 10px 15px;
    margin-top: 20px;
}
.goods-price{
    color:#f60;
}
.product-spec{
    height: 100px;
    overflow: scroll;
    padding: 0 15px;
}
.spec-select{
    font-size: 14px;
    margin-bottom: 10px;
}
.spec-title{
    /*min-width: 70px;*/
}
.spec-options{
    display: flex;

}
.spec-select{
    
}
.spec-head{
    height: 30px;
    padding: 10px;
    text-align: center;
    overflow: hidden;

}
.spec-footer{
    height: 45px;
    border-top: 1px solid #ccc;
    padding:0 0 0 15px;
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    align-items: center;
}
.goods-buyprice{
    color:#f60;
}
.confirm-btn{
    width: 100px;
    height: 45px;
    line-height: 45px;
    color:#ffffff;
    text-align: center;    
    background-color: #E64340;
    font-size: 15px;
}
.spec-options{
    flex-wrap: wrap;
    justify-content: flex-start;
}
.spec-item{
    border: 1px #ccc solid;
    margin: 0 8px 8px 0;
    padding: 5px 8px;
    display: flex;
    align-items: center;

}
.spec-item.active{
    border-color:#f60;
    color:#f60;
}
.spec-item.disabled{
    border-style: dashed;
    color: #ccc;
}
.cartamount{
    width: 50%;
    text-align: left;
    color: #f60;
    font-size: 15px;
}