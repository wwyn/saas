page{
    background: #efefef;
    font-size: 14px;
}
.order-item-panel{

    display: flex;
    align-items: center;
    flex-direction: row;
    padding: 10px 15px 5px 15px;
    background: #f9f9f9;
}

.item-desc{
    font-size: 13px;
}
.item-image{
    width: 45px;
    height: 75px;
    margin-right: 5px;
}
.logi-logo{
    width: 120px;
    height: 32px;
}
.item-image image{
    width: 45px;
    height: 45px;
    border: 1px #e9e9e9 solid;
    border-radius: 2px;
}
.item-image .sendnum{
    font-size: 9px;
    color: #666;
    text-align: center;
}

.logistics-logs .time{
    font-size: 12px;
    line-height: 25px;
    color: #666;
}
